view {
    color: var(--themeColor);
}

.index-swiper swiper {
    width: 750rpx;
    height: 340rpx;
}

.index-swiper swiper image {
    width: 100%;
}

.index-cate {
    display: flex;
}

.index-cate navigator {
    padding: 20rpx;
    flex: 1;
}

.index-cate navigator image {
    width: 100%;
}

.index-floor .floor-title {
    padding: 10rpx 0;
}

.index-floor .floor-title image {
    width: 100%;
}

.index-floor .floor-content {
    overflow: hidden;
}

.index-floor .floor-content navigator {
    float: left;
    width: 33.33%;
}

.index-floor .floor-content navigator:nth-last-child(4) {
    height: 27.72711207vw;
}

.index-floor .floor-content navigator:nth-child(2),
.index-floor .floor-content navigator:nth-child(4) {
    border: 10rpx solid #fff;
}

.index-floor .floor-content navigator image {
    width: 100%;
    height: 100%;
}